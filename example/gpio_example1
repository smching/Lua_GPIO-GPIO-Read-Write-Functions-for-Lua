#!/usr/bin/lua
require "gpio"
pinMode(20,OUTPUT)
pinMode(19,OUTPUT)
pinMode(18,OUTPUT)
pinMode(22,OUTPUT)
pinMode(21,OUTPUT)

digitalWrite(20,HIGH)
delay(0.2)
digitalWrite(19,HIGH)
delay(0.2)
digitalWrite(18,HIGH)
delay(0.2)
digitalWrite(22,HIGH)
delay(0.2)
digitalWrite(21,HIGH)
delay(0.2)

digitalWrite(20,LOW)
delay(0.2)
digitalWrite(19,LOW)
delay(0.2)
digitalWrite(18,LOW)
delay(0.2)
digitalWrite(22,LOW)
delay(0.2)
digitalWrite(21,LOW)
